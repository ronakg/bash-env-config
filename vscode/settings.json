{
  "[go]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": true,
      "source.organizeImports": true
    },
    "editor.formatOnSave": true
  },
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
  "breadcrumbs.symbolPath": "last",
  "cSpell.diagnosticLevel": "Hint",
  "cSpell.userWords": [
    "DNAC",
    "Debugf",
    "Errorw",
    "RBAC",
    "bodyf",
    "fqdn",
    "maglev",
    "pxgrid",
    "resty",
    "ronak"
  ],
  "codeui.targetingMode": "general",
  "customizeUI.activityBar": "bottom",
  "customizeUI.titleBar": "inline",
  "debug.console.fontFamily": "FiraCode-Retina",
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.renderSideBySide": true,
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.cursorBlinking": "smooth",
  "editor.cursorSurroundingLines": 4,
  "editor.folding": false,
  "editor.fontFamily": "FiraCode-Retina, Fira Code, Consolas, Menlo",
  "editor.fontLigatures": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.gotoLocation.alternativeReferenceCommand": "editor.action.goToDefinition",
  "editor.lightbulb.enabled": false,
  "editor.minimap.enabled": false,
  "editor.minimap.renderCharacters": false,
  "editor.mouseWheelZoom": true,
  "editor.renderLineHighlightOnlyWhenFocus": true,
  "editor.renderWhitespace": "boundary",
  "editor.scrollbar.verticalScrollbarSize": 0,
  "editor.smoothScrolling": true,
  "editor.suggest.localityBonus": true,
  "editor.suggestSelection": "first",
  "editor.wrappingIndent": "none",
  "errorLens.addAnnotationTextPrefixes": true,
  "errorLens.followCursor": "activeLine",
  "errorLens.fontStyleItalic": true,
  "errorLens.gutterIconSet": "defaultOutline",
  "errorLens.gutterIconSize": "70%",
  "errorLens.gutterIconsEnabled": true,
  "errorLens.onSave": true,
  "errorLens.statusBarColorsEnabled": true,
  "errorLens.statusBarMessageEnabled": true,
  "explorer.openEditors.visible": 0,
  "explorer.sortOrder": "type",
  "extensions.ignoreRecommendations": false,
  "files.exclude": {
    "**/*.class": true,
    "**/*.git": true,
    "**/.DS_Store": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/.settings": true
  },
  "files.hotExit": "off",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "git.autofetch": true,
  "git.countBadge": "tracked",
  "git.enableSmartCommit": true,
  "git.ignoreLimitWarning": true,
  "git.showCommitInput": false,
  "git.smartCommitChanges": "tracked",
  "git.untrackedChanges": "hidden",
  "gitlens.advanced.telemetry.enabled": false,
  "gitlens.blame.heatmap.enabled": false,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "go.autocompleteUnimportedPackages": true,
  "go.buildOnSave": "workspace",
  "go.formatTool": "goimports",
  "go.gotoSymbol.includeImports": true,
  "go.lintOnSave": "package",
  "go.useLanguageServer": true,
  "gopls": {
    "analyses": {
      "fillreturns": true,
      "nonewvars": true,
      "undeclaredname": true,
      "unreachable": true,
      "unusedparams": true
    },
    "codelens": {
      "test": true
    },
    "diagnostics": true,
    "documentLink": true,
    "usePlaceholders": true
  },
  "gruvboxMaterial.darkContrast": "hard",
  "gruvboxMaterial.darkSelection": "blue",
  "gruvboxMaterial.darkWorkbench": "flat",
  "gruvboxMaterial.italicKeywords": true,
  "java.codeGeneration.generateComments": true,
  "java.codeGeneration.hashCodeEquals.useInstanceof": true,
  "java.codeGeneration.toString.codeStyle": "STRING_FORMAT",
  "java.codeGeneration.useBlocks": true,
  "java.completion.guessMethodArguments": true,
  "java.debug.settings.console": "internalConsole",
  "java.debug.settings.forceBuildBeforeLaunch": false,
  "java.debug.settings.hotCodeReplace": "auto",
  "java.debug.settings.showQualifiedNames": true,
  "java.debug.settings.showStaticVariables": true,
  "java.dependency.showMembers": true,
  "java.format.settings.url": "file:///Users/rogandhi/work/dev-tools/eclipse-java-formatter.xml",
  "java.implementationsCodeLens.enabled": true,
  "java.jdt.ls.vmargs": "-XX:+UseParallelGC -Xms5120m -Xmx5120m -noverify",
  "java.requirements.JDK11Warning": false,
  "java.semanticHighlighting.enabled": true,
  "java.signatureHelp.enabled": true,
  "java.test.report.showAfterExecution": "never",
  "leetcode.hint.commandShortcut": false,
  "leetcode.hint.configWebviewMarkdown": false,
  "leetcode.workspaceFolder": "/Users/rogandhi/Google Drive/Projects/leetcode",
  "maven.executable.options": "-DskipTests",
  "problems.showCurrentInStatus": true,
  "python.experiments.enabled": false,
  "python.languageServer": "Microsoft",
  "scm.inputFontFamily": "editor",
  "search.useGlobalIgnoreFiles": true,
  "sonarlint.disableTelemetry": true,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.experimentalUseTitleEvent": true,
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.scrollback": 5000,
  "terminal.integrated.shell.linux": "/bin/bash",
  "terminal.integrated.shell.osx": "/usr/local/bin/bash",
  "timeline.excludeSources": [
    "git-history"
  ],
  "vim.enableNeovim": true,
  "vim.gdefault": true,
  "vim.highlightedyank.enable": true,
  "vim.hlsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "after": [
        "<Esc>"
      ],
      "before": [
        "j",
        "j"
      ]
    }
  ],
  "vim.leader": "<space>",
  "vim.neovimPath": "/usr/local/bin/nvim",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "s"
      ],
      "commands": [
        "workbench.action.gotoSymbol"
      ]
    },
    {
      "before": [
        "b"
      ],
      "commands": [
        "workbench.action.showAllEditorsByMostRecentlyUsed"
      ]
    },
    {
      "before": [
        "<leader>",
        "t"
      ],
      "commands": [
        "workbench.files.action.focusFilesExplorer"
      ]
    },
    {
      "before": [
        "<C-n>"
      ],
      "commands": [
        ":nohl"
      ]
    },
    {
      "before": [
        "u"
      ],
      "commands": [
        "undo"
      ]
    },
    {
      "before": [
        "<C-r>"
      ],
      "commands": [
        "redo"
      ]
    },
    {
      "before": [
        "<leader>",
        "f"
      ],
      "commands": [
        "revealInExplorer"
      ]
    },
    {
      "before": [
        "w"
      ],
      "commands": [
        "workbench.action.files.save"
      ]
    },
    {
      "before": [
        "W"
      ],
      "commands": [
        "workbench.action.files.saveAll"
      ]
    },
    {
      "before": [
        "<leader>",
        "p",
        "d"
      ],
      "commands": [
        "editor.action.peekDefinition"
      ]
    },
    {
      "before": [
        "<leader>",
        "p",
        "c"
      ],
      "commands": [
        "editor.showCallHierarchy"
      ]
    },
    {
      "before": [
        "g",
        "r"
      ],
      "commands": [
        "editor.action.referenceSearch.trigger"
      ]
    },
    {
      "before": [
        "g",
        "i"
      ],
      "commands": [
        "editor.action.goToImplementation"
      ]
    },
    {
      "before": [
        "e"
      ],
      "commands": [
        "workbench.action.closeActiveEditor"
      ]
    },
    {
      "after": [
        "<C-o>"
      ],
      "before": [
        "<BS>"
      ]
    },
    {
      "before": [
        "K"
      ],
      "commands": [
        "editor.action.showHover"
      ]
    },
    {
      "before": [
        "<leader>",
        "r",
        "n"
      ],
      "commands": [
        "editor.action.rename"
      ]
    },
    {
      "before": [
        "]",
        "c"
      ],
      "commands": [
        "editor.action.marker.nextInFiles"
      ]
    },
    {
      "before": [
        "[",
        "c"
      ],
      "commands": [
        "editor.action.marker.prevInFiles"
      ]
    }
  ],
  "vim.replaceWithRegister": true,
  "vim.showcmd": false,
  "vim.sneak": true,
  "vim.textwidth": 100,
  "vim.useSystemClipboard": true,
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [
        ">"
      ],
      "commands": [
        "editor.action.indentLines"
      ]
    },
    {
      "before": [
        "<"
      ],
      "commands": [
        "editor.action.outdentLines"
      ]
    },
    {
      "after": [
        "p",
        "g",
        "v",
        "y"
      ],
      "before": [
        "p"
      ]
    }
  ],
  "vim.visualstar": true,
  "vim.whichwrap": "h,l,<,>,[,]",
  "vsicons.dontShowNewVersionMessage": true,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.newWindowDimensions": "maximized",
  "window.restoreWindows": "none",
  "window.title": "${dirty} ${activeEditorMedium}${separator}${rootName}",
  "window.titleBarStyle": "native",
  "window.zoomLevel": 0,
  "workbench.colorCustomizations": {
    "tab.activeBorder": "#ff0000"
  },
  "workbench.colorTheme": "Gruvbox Material Dark",
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.fontAliasing": "antialiased",
  "workbench.list.smoothScrolling": true,
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "welcomePageInEmptyWorkbench",
  "workbench.statusBar.visible": true
}
