    "leetcode.hint.configWebviewMarkdown": false,
    "leetcode.workspaceFolder": "/Users/rogandhi/Google Drive/Projects/leetcode",
    "maven.executable.options": "-DskipTests -nsu",
    "maven.executable.preferMavenWrapper": false,
    "problems.showCurrentInStatus": true,
    "python.jediEnabled": false,
    "search.quickOpen.includeSymbols": true,
    "sonarlint.connectedMode.connections.sonarqube": [
        {
            "serverUrl": "https://engci-sonar-sjc.cisco.com/sonar",
            "token": "16ce9543236ff33f3d686dd4c8f7a1ee6cd09aa6"
        }
    ],
    "sonarlint.disableTelemetry": true,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.experimentalUseTitleEvent": true,
    "terminal.integrated.scrollback": 5000,
    "terminal.integrated.shell.linux": "/bin/bash",
    "terminal.integrated.shell.osx": "/usr/local/bin/bash",
    "timeline.excludeSources": [
        "git-history"
    ],
    "vim.enableNeovim": true,
    "vim.gdefault": true,
    "vim.highlightedyank.enable": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
        {
            "after": [
                "<Esc>"
            ],
            "before": [
                "j",
                "j"
            ]
        }
    ],
    "vim.leader": "<space>",
    "vim.neovimPath": "/usr/local/bin/nvim",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "t"
            ],
            "commands": [
                "workbench.files.action.focusFilesExplorer"
            ]
        },
        {
            "before": [
                "<C-n>"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                "u"
            ],
            "commands": [
                "undo"
            ]
        },
        {
            "before": [
                "<leader>",
                "f"
            ],
            "commands": [
                "revealInExplorer"
            ]
        },
        {
            "before": [
                "w"
            ],
            "commands": [
                "workbench.action.files.save"
            ]
        },
        {
            "before": [
                "<leader>",
                "p",
                "d"
            ],
            "commands": [
                "editor.action.peekDefinition"
            ]
        },
        {
            "before": [
                "<leader>",
                "p",
                "c"
            ],
            "commands": [
                "editor.showCallHierarchy"
            ]
        },
        {
            "before": [
                "g",
                "c"
            ],
            "commands": [
                "references-view.showCallHierarchy"
            ]
        },
        {
            "before": [
                "g",
                "r"
            ],
            "commands": [
                "editor.action.referenceSearch.trigger"
            ]
        },
        {
            "before": [
                "g",
                "i"
            ],
            "commands": [
                "editor.action.goToImplementation"
            ]
        },
        {
            "before": [
                "e"
            ],
            "commands": [
                "workbench.action.closeActiveEditor"
            ]
        },
        {
            "after": [
                "<C-o>"
            ],
            "before": [
                "<BS>"
            ]
        },
        {
            "before": [
                "g",
                "r"
            ],
            "commands": [
                "editor.action.referenceSearch.trigger"
            ]
        },
        {
            "before": [
                "<leader>",
                "r",
                "n"
            ],
            "commands": [
                "editor.action.rename"
            ]
        },
        {
            "before": [
                "<leader>",
                "p"
            ],
            "commands": [
                "workbench.action.gotoSymbol"
            ]
        },
        {
            "before": [
                "]",
                "c"
            ],
            "commands": [
                "editor.action.marker.nextInFiles"
            ]
        },
        {
            "before": [
                "[",
                "c"
            ],
            "commands": [
                "editor.action.marker.prevInFiles"
            ]
        }
    ],
    "vim.replaceWithRegister": true,
    "vim.sneak": true,
    "vim.textwidth": 120,
    "vim.useSystemClipboard": true,
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                ">"
            ],
            "commands": [
                "editor.action.indentLines"
            ]
        },
        {
            "before": [
                "<"
            ],
            "commands": [
                "editor.action.outdentLines"
            ]
        },
        {
            "after": [
                "p",
                "g",
                "v",
                "y"
            ],
            "before": [
                "p"
            ]
        }
    ],
    "vim.visualstar": true,
    "vsicons.dontShowNewVersionMessage": true,
    "window.newWindowDimensions": "maximized",
    "window.title": "${dirty} ${activeEditorMedium}${separator}${rootName}",
    "window.titleBarStyle": "native",
    "workbench.colorCustomizations": {
        "tab.activeBorder": "#ff0000"
    },
    "workbench.colorTheme": "Gruvbox Material Dark",
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.fontAliasing": "auto",
    "workbench.sideBar.location": "right",
    "workbench.startupEditor": "welcomePageInEmptyWorkbench"
}
