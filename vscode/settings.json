{
    "[xml]": {
        "editor.defaultFormatter": "redhat.vscode-xml"
    },
    "breadcrumbs.enabled": true,
    "breadcrumbs.symbolPath": "last",
    "cSpell.diagnosticLevel": "Hint",
    "cSpell.userWords": [
        "AUTHZ",
        "Gson",
        "Mockito",
        "PUBSUB",
        "Pxgrid",
        "Rbac",
        "Ronak",
        "dxhub",
        "itertools",
        "messageservice",
        "spog",
        "xgrid",
        "xmpp"
    ],
    "customizeUI.activityBar": "bottom",
    "customizeUI.titleBar": "inline",
    "diffEditor.ignoreTrimWhitespace": false,
    "diffEditor.renderSideBySide": true,
    "editor.cursorBlinking": "smooth",
    "editor.folding": false,
    "editor.fontFamily": "FiraCode-Retina,Fira Code, Consolas, Menlo",
    "editor.fontLigatures": true,
    "editor.formatOnPaste": true,
    "editor.formatOnType": true,
    "editor.hideCursorInOverviewRuler": true,
    "editor.lightbulb.enabled": false,
    "editor.matchBrackets": "near",
    "editor.minimap.enabled": false,
    "editor.minimap.renderCharacters": false,
    "editor.mouseWheelZoom": true,
    "editor.overviewRulerBorder": false,
    "editor.renderLineHighlightOnlyWhenFocus": true,
    "editor.scrollbar.verticalScrollbarSize": 0,
    "editor.smoothScrolling": true,
    "editor.suggest.localityBonus": true,
    "editor.suggest.shareSuggestSelections": true,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "on",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "comment",
                "settings": {
                    "fontStyle": "italic"
                }
            }
        ]
    },
    "editor.wordWrap": "on",
    "errorLens.addAnnotationTextPrefixes": true,
    "errorLens.followCursor": "activeLine",
    "errorLens.fontStyleItalic": true,
    "errorLens.gutterIconSet": "defaultOutline",
    "errorLens.gutterIconSize": "70%",
    "errorLens.gutterIconsEnabled": true,
    "errorLens.onSave": true,
    "errorLens.statusBarColorsEnabled": true,
    "errorLens.statusBarMessageEnabled": true,
    "explorer.openEditors.visible": 0,
    "explorer.sortOrder": "type",
    "files.exclude": {
        "**/*.class": true,
        "**/*.git": true,
        "**/.DS_Store": true,
        "**/.classpath": true,
        "**/.factorypath": true,
        "**/.project": true,
        "**/.settings": true
    },
    "files.hotExit": "off",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "git.autofetch": true,
    "git.countBadge": "tracked",
    "git.ignoreLegacyWarning": true,
    "gitlens.advanced.maxSearchItems": 100,
    "gitlens.advanced.telemetry.enabled": false,
    "gitlens.blame.heatmap.enabled": false,
    "gitlens.codeLens.enabled": false,
    "gitlens.codeLens.recentChange.enabled": false,
