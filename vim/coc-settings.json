{
    "suggest.enablePreview": true,
    "codeLens.enable": true,
    "git.addedSign.text": "┃",
    "git.removedSign.text": "┃",
    "git.changedSign.text": "┃",
    "git.changeRemovedSign.text": "┃",
    "git.addGBlameToBufferVar": true,
    "diagnostic.maxWindowHeight": 20,
    "diagnostic.errorSign": "✖",
    "diagnostic.warningSign": "⚠",
    "diagnostic.infoSign": "●",
    "diagnostic.hintSign": "○",
    "java.jdt.ls.vmargs": "-noverify -Xms4096m -Xmx5120m -XX:+UseG1GC -XX:+UseStringDeduplication",
    "java.progressReports.enabled": true,
    "java.completion.guessMethodArguments": true,
    "suggest.maxCompleteItemCount": 10,
    "suggest.minTriggerInputLength": 2,
    "go.goplsOptions": {
        "deepCompletion": true,
        "completeUnimported": true
    },
    "diagnostic.checkCurrentLine": true,
    "diagnostic.locationlist": false,
    "snippets.autoTrigger": true,
    "suggest.noselect": true,
    "snippets.shortcut": "~",
    "yaml.format.enable": true,
    "yaml.completion": true,
    "yaml.validate": true,
    "python.jediEnabled": false
}
